---
import Layout from "../layouts/Layout.astro";
import JobCard from "../components/JobCard.astro";
---

<Layout title="Karriera" header="Karriera juaj tek ne" img="/img/karriera.jpg">
  <div class="mx-auto flex w-2/3 flex-col gap-6 py-20">
    {
      () => {
        const parts = [];
        for (let i = 0; i < 10; i++) {
          parts.push(<JobCard title="Inxhinier i pajisjeve te baterive" />);
        }
        return parts;
      }
    }
    <JobCard title="Inxhinier i pajisjeve te baterive" />
  </div>
</Layout>
